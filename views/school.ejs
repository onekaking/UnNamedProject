<div class="container">
  <h1>Danh sách trường <a class="btn btn-default" href="/school/add">Tạo mới</a></h1>
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Tên trường</th>
        <th>Địa chỉ</th>
        <th>Ngày cập nhật</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% schools.forEach( function(ele, index) { %> 
        <tr>
          <td><%= ++index %></td>
          <td><%= ele.name %></td>
          <td><%= ele.address %></td>
          <td><%= ele.updatedAt %></td>
          <td><a href="/school/delete/<%= ele.id %>"><i class="glyphicon glyphicon-remove"></i></a></td>
        </tr>
      <% }); %>
    </tbody>
</table>
</div>
